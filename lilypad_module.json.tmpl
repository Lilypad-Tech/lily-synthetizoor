{
"machine": {
      "gpu": 0,
      "cpu": 1000,
      "ram": 100
  },
  "job": {
    "APIVersion": "V1beta1",
    "Spec": {
      "Deal": {
        "Concurrency": 1
      },
      "Docker": {
       "Entrypoint": [
        "python", 
        "app.py",
        {{.URL}},
        {{.SENTENCE}},
        {{.LANGUAGE}},
        {{.INPUT_FOLDER}},
        {{.OUTPUT_FOLDER}}
        ],
        "Image": "bringhi/synt@sha256:31def637950bfafd5f9568dcdab359bb7524e3362d084a51d2832553df621626"
      },
      "Engine": "Docker",
      "Network": {
        "Type": "None"
      },
      "PublisherSpec": {
        "Type": "Estuary"
      },
      "Resources": {
        "GPU": ""
      },
      "Timeout": 4800,
      "Verifier": "Noop",
      "inputs": [
          {
              "URL": {{.URL}},
              "Name": "fabri.wav",
              "StorageSource": "urlDownload",
              "path": "/inputs"
          }
      ],
      "outputs": [
          {
              "StorageSource": "IPFS",
              "Name": "outputs",
              "path": "/outputs"
          }
      ]
    }
  }
}

