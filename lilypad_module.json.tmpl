{
"machine": {
      "gpu": 0,
      "cpu": 1000,
      "ram": 100
  },
  "job": {
    "APIVersion": "V1beta1",
    "Spec": {
      "Deal": {
        "Concurrency": 1
      },
      "Docker": {
       "Entrypoint": [
        "python", 
        "app.py",
        {{.URL}},
        {{.SENTENCE}},
        {{.LANGUAGE}},
        {{.INPUT_FOLDER}},
        {{.OUTPUT_FOLDER}}
        ],
        "Image": "bringhi/synt@sha256:41ac8e9ffd606390ae48170d2699c3003cc270fcbebc5266e770ebd6a0b1d237"
      },
      "Engine": "Docker",
      "Network": {
        "Type": "None"
      },
      "PublisherSpec": {
        "Type": "Estuary"
      },
      "Resources": {
        "GPU": ""
      },
      "Timeout": 1800,
      "Verifier": "Noop",
      "inputs": [
          {
              "URL": {{.URL}},
              "Name": "fabri.wav",
              "StorageSource": "urlDownload",
              "path": "/inputs"
          }
      ],
      "outputs": [
          {
              "StorageSource": "IPFS",
              "Name": "outputs",
              "path": "/outputs"
          }
      ]
    }
  }
}

