{
  "machine": {
    "gpu": 0,
    "cpu": 1000,
    "ram": 100
  },
  "job": {
    "APIVersion": "V1beta1",
    "Spec": {
      "Deal": {
        "Concurrency": 1
      },
      "Docker": {
       "Entrypoint": [
        "python", 
        "app.py",
        {{.URL}}
        ],
        "Image": "bringhi/synt@sha256:d3793fb5250cc9f2ca431c0828df21f7b2a5f6a1bd0b753acbecc8f6ab3cb09b"
      },
      "Engine": "Docker",
      "Network": {
        "Type": "None"
      },
      "PublisherSpec": {
        "Type": "Estuary"
      },
      "Resources": {
        "GPU": ""
      },
      "Timeout": 1800,
      "Verifier": "Noop",
      "inputs": [
          {
              "URL": {{.URL}},
              "Name": "cane.txt",
              "StorageSource": "urlDownload",
              "path": "/inputs"
          }
      ],
      "outputs": [
          {
              "StorageSource": "IPFS",
              "Name": "outputs",
              "path": "/outputs"
          }
      ]
    }
  }
}

